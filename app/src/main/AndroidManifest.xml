<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cosine.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".setAlarmActivity" />
        <activity android:name=".AlarmRingingActivity" />
        <activity android:name=".AlarmListActivity" />
        <activity android:name=".GameModeActivity" />
        <activity android:name=".JumpActivity" />
        <activity android:name=".JumpScoreActivity" />
        <activity
            android:name=".maze.MazeActivity"
            android:theme="@style/AppTheme.MazeTheme" />
        <activity android:name=".ChallengeSwitchingActivity"></activity>
    </application>

</manifest>